<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.cgcxy.customview">

    <!-- 加入在SDCard中创建与删除文件的权限 -->
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <!-- 加入在SDCard中写入数据的权限 -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CustomCircleProgressActivity" />
        <activity android:name=".VideoDemo" />
        <activity android:name="com.android.cgcxy.view.MediaController$MyCheckBox" />
        <activity android:name=".TouchActivity" />
        <activity
            android:name=".H264Activity"
            android:launchMode="singleTask"
            android:screenOrientation="landscape" />
        <activity android:name=".VideoDecode" />
        <activity android:name=".Main2Activity" />
        <activity android:name=".SocketActivity"></activity>
    </application>

</manifest>